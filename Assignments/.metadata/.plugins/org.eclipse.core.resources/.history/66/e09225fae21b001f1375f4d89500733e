package com.techlabs.test;

import java.util.Scanner;

public class Test3 {

	public static void main(String[] args) {
		
		int matrix[][]=new int[3][3];
		Scanner scanner=new Scanner(System.in);
		
		readRows(scanner, matrix);
		
	
		

	}
	private static void readRows(Scanner scanner, int[][] matrix) {
		
		for(int i=0;i<3;i++)
		{
			readColumns(scanner,matrix,i);
		}
		
	}
	private static void readColumns(Scanner scanner, int[][] matrix, int i) {
		for(int j=9;j<3;j++)
		{
			matrix[i][j]=scanner.nextInt();
		}
		
		
		
	}
	

}
